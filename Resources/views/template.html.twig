    {% endjavascripts %}
    <script type="text/javascript">
        $(function () {
            //fancyBox
            $(document).on("click", ".cs-coin-slider", function () {
                $href = $(this).attr('href');
                $index = $('.fancybox[href="' + $href + '"]').index();
                $.fancybox.open($('.fancybox'), {'index': $index});
                return false;
            });
            // Slider
            $('#coin-slider').coinslider({width: 935, height: 285, opacity: 1, navigation: true, next: '{% trans %}next{% endtrans %}', prev: '{% trans %}prev{% endtrans %}', links: true});


        });
    </script>
{% endblock javascripts %}
